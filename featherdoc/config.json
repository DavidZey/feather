{
  "navbar": [
    {
      "url": "#",
      "text": "Introduction",
      "label": "introduction",
      "onclick": "$('html, body').animate({ scrollTop: $('.introduction').offset().top - 60 }, 'slow' );"
    }
  ],
  "socket.io": {
    "enabled": false
  },
  "ui": {
    "enabled": true,
    "provider": {
      "name": "bootstrap"      
    }
  },
  "logging": {
    "enabled": true,
    "defaultLevel": "info",
    "absorbConsoleLogging": true,
    "appenders": [
      {
        "type": "console",
        "options": { 
          "layout": "colored",
          "disabled": true,
          "excludedCategories": ["http.access"]
        }
      },
      {
        "type": "file",
        "options": {
          "layout": "basic",
          "filename": "logs/access.log",
          "maxFileSize": 1024,
          "numBackups": 10,
          "filePollInterval": 60,
          "includedCategories": ["http.access"]
        }
      }
    ]
  }
}
